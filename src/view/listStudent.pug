doctype html
html
  head
    title List of Students
  body
    h1 Students
    table
        thead
            tr
                th Nom
                th Date de naissance
                th Supprimer
        tbody
            each student in students
                tr
                    td #{student.name}
                    td #{student.birth}
                    td 
                        a(href="deleteStudent/"+name) X
